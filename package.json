{
  "name": "j2-middleware-starter-kit",
  "version": "1.0.0",
  "description": "A boilerplate for Node.js conduit services",
  "keywords": [
    "node",
    "es6",
    "express",
    "mssql",
    "oracle",
    "docker",
    "microservice"
  ],
  "repository": {
    "type": "git",
    "url": "https://faizanaion@bitbucket.org/aiondigitalteam/conduit.bp.api.git"
  },
  "author": {
    "name": "Faizan Ahmad",
    "email": "faizan@aiondigital.com"
  },
  "contributors": [
    "Usman Rana <usman@aiondigital.com>"
  ],
  "main": "bin/start.js",
  "scripts": {
    "start": "npm run start:prod",
    "build": "babel ./ --out-dir dist --ignore 'node_modules/**/*.js'",
    "server": "babel-node ./bin/start.js",
    "server:prod": "node ./dist/bin/start.js",
    "start:dev": "NODE_ENV=development npm-run-all server",
    "start:prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "lint": "eslint \"**/*.js\"",
    "clean": "rimraf dist",
    "precommit": "lint-staged",
    "prettify": "tsfmt -r --baseDir ./"
  },
  "lint-staged": {
    "*": [
      "prettify",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "http-status": "^1.4.0",
    "joi": "^14.3.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "mssql": "^5.1.0",
    "nconf": "^0.10.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "tedious": "^6.6.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "husky": "^3.0.9",
    "lint-staged": "^10.0.0-1",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": "12.13.0",
    "npm": "~6.12.0"
  },
  "license": "ISC"
}
